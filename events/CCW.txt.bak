				}
			}
			every_state = {
				limit = {
					is_SK_state = yes
					#is_controlled_by = NKR
				}
				
				SKR = {
					transfer_state = PREV
				}
			}
			else_if = {
				limit = {
					SKT = {
						exists = yes
						NOT = {
							has_war_with = ROOT
						}
					}
					NOT = {
						country_exists = SKR
					}
				}
				every_state = {
					limit = {
						is_SK_state = yes
						#is_controlled_by = NKR
					}	
					SKT = {
						transfer_state = PREV
					}
				}
			}
